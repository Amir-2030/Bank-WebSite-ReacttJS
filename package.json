{
  
  "name": "bankwebsite",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
  "bootstrap": "^5.1.3",
  "gh-pages": "^3.2.3",
  
    
  "react": "^16.6.3",
  
    
  "react-bootstrap": "^2.1.2",
  
    
  "react-dom": "^16.6.3",
  
    
  "react-router-dom": "^6.2.1",
  
    
  "react-scripts": "^5.0.0"
  
  },
  
  "scripts": {
  
    
  "start": "react-scripts start",
  
    
  "build": "react-scripts build",
  
    
  "test": "react-scripts test",
  
    
  "eject": "react-scripts eject"
  
  },
  
  "eslintConfig": {
  
    
  "extends": "react-app"
  
  },
  
  "browserslist": [
  
    
  ">0.2%",
  
    
  "not dead",
  
    
  "not ie <= 11",
  
    
  "not op_mini all"
  
  ],
  
  "devDependencies": {
  
    
  "@testing-library/react": "^12.1.3",
  
    
  "eslint": "^8.10.0",
  
    
  "eslint-plugin-react": "^7.29.2",
  
    
  "web-vitals": "^2.1.4"
  
  },
  
  "plugins": [
  
    ["@babel/plugin-transform-react-jsx", {
      "runtime": "automatic"
    }],
  "react"
  
  ],
 
  
  "rules": {
    "no-unused-vars": "off",
    "react/jsx-uses-react": "error",   
   "react/jsx-uses-vars": "error" ,
    
 
  
    
  "jsx-a11y/anchor-is-valid": [
  
    
    
  "error",
  
    
    
  {
  
    
    
    
  "components": [
  
    
    
    
    
  "Link"
  
    
    
    
  ],
  
    
    
    
  "specialLink": [
  
    
    
    
    
  "hrefLeft",
  
    
    
    
    
  "hrefRight"
  
    
    
    
  ],
  
    
    
    
  "aspects": [
  
    
    
    
    
  "noHref",
  
    
    
    
    
  "invalidHref",
  
    
    
    
    
  "preferButton"
  
    
    
    
  ]
  
    
    
  }
  
    
  ],
  
    
  "react/jsx-no-duplicate-props": [
  
    
    
  1,
  
    
    
  {
  
    
    
    
  "ignoreCase": false
  
    
    
  }
  
    
  ]
  
  },
  
  "no-unused-vars": [
  
    
  "error",
  
    
  {
  
    
    
  "varsIgnorePattern": "^h$"
  
    
  }
  
  ]
}
